import cv2
import numpy as np

#cX_r:23,cY_r:7,w_r:46,h_r:19,cX_l:18,cY_l:7,w_l:44,h_l:20
#cX_r:24,cY_r:11,w_r:45,h_r:23,cX_l:20,cY_l:7,w_l:40,h_l:22
blank_image_r = np.zeros((23,45,3), np.uint8)
blank_image_l = np.zeros((22,40,3), np.uint8)
cv2.circle(blank_image_r, (24, 11), 1, (0, 255, 0), -1)  
cv2.circle(blank_image_l, (20, 7), 1, (0, 255, 0), -1)

cv2.imshow("right eye",blank_image_r)

cv2.imshow("left eye",blank_image_l)

cv2.waitKey(0)
cv2.destroyAllWindows()